{
  "name": "kevoree-comp-mqttpubclient",
  "version": "2.0.2",
  "main": "./lib/MQTTPubClient.js",
  "description": "Publishes the messages it receives to its input ports to the specified MQTT <strong>host</strong>:<strong>port</strong> and <strong>topic</strong>",
  "kevoree": {
    "namespace": "kevoree"
  },
  "scripts": {
    "prepublish": "npm run lint && grunt",
    "postpublish": "grunt publish",
    "lint": "eslint lib"
  },
  "keywords": [
    "kevoree-std-lib",
    "kevoree",
    "library",
    "comp"
  ],
  "dependencies": {
    "kevoree-entities": "^9.2.1",
    "mqtt": "^2.1.3"
  },
  "devDependencies": {
    "eslint": "^3.11.1",
    "grunt": "^1.0.1",
    "grunt-kevoree": "^6.0.0",
    "grunt-kevoree-genmodel": "^3.0.3",
    "grunt-kevoree-registry": "^4.0.0",
    "load-grunt-tasks": "^3.5.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ]
  }
