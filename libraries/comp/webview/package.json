{
  "name": "kevoree-comp-webview",
  "version": "0.0.5",
  "description": "Displays a Website in a Kevoree Browser Runtime",
  "main": "lib/Webview.js",
  "kevoree": {
    "namespace": "kevoree",
    "browser": "lib/browser.js"
  },
  "scripts": {
    "build": "kevoree-scripts build",
    "prestart": "npm run build",
    "start": "kevoree-scripts start",
