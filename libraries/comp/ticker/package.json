{
  "name": "kevoree-comp-ticker",
  "version": "5.3.3",
  "description": "A Kevoree component that sends a 'tick' message at user-defined intervals",
  "main": "src/Ticker.js",
  "kevoree": {
    "namespace": "kevoree",
    "browser": "src/browser/main.js"
  },
  "scripts": {
    "lint": "eslint lib browser.js test",
    "webpack": "webpack -p --progress",
    "prebuild": "npm run lint",
    "build": "kevoree-scripts build",
    "postbuild": "npm run webpack",
    "prestart": "npm run build",
    "start": "kevoree-scripts start",
    "prekev:publish": "npm run build",
    "kev:publish": "kevoree-scripts publish",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run kev:publish"
  },
  "keywords": [
    "kevoree-std-lib",
    "kevoree",
    "library",
    "comp"
  ],
  "dependencies": {
    "kevoree-entities": "^9.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.18.0",
    "eslint-config-standard-jsx": "^3.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.7.1",
    "expect": "^1.20.2",
    "json-loader": "^0.5.4",
    "kevoree-scripts": "^1.0.0",
    "mocha": "^3.4.2",
    "webpack": "^3.4.1",
    "react": "^15.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kevoree/kevoree-js-comp-ticker.git"
  },
  "author": "Maxime Tricoire <max.tricoire@gmail.com>",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/kevoree/kevoree-js-comp-ticker/issues"
  },
  "homepage": "https://github.com/kevoree/kevoree-js-comp-ticker"
}
