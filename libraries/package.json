{
  "name": "kevoree-js-libs",
  "version": "0.0.0",
  "description": "KevoreeJS dev env: installs needed tools for dev scripts",
  "main": "update.js",
  "author": "Maxime Tricoire <max.tricoire@gmail.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "async": "^2.0.1",
    "chalk": "^1.1.3",
    "eslint": "^3.12.2",
    "grunt": "^1.0.1",
    "indent-string": "^3.0.0",
    "ora": "^0.2.3",
    "time-grunt": "^1.4.0"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
		"parserOptions": {
			"ecmaVersion": 6
		},
    "extends": [
      "eslint:recommended"
    ],
		"rules": {
			"semi": 2,
      "no-use-before-define": 2,
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "always",
          "named": "never",
          "asyncArrow": "ignore"
        }
      ]
		}
  }
}
