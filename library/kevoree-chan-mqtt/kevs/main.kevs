add node0 : JavascriptNode
add sync : WSGroup

add node0.console : ConsolePrinter
add node0.ticker : Ticker
add myChan : MQTTChannel

attach node0 sync

set sync.master = 'node0'

bind node0.console.input myChan
bind node0.ticker.tick myChan

network node0.lo.ip 127.0.0.1
set node0.logLevel = 'DEBUG'