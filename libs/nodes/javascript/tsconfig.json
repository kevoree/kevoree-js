{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "built"
    },
    "filesGlob": [
        "src/**/*.ts",
        "!node_modules/**"
    ],
    "files": [
        "src/main/JavascriptNode.ts",
        "src/test/ModelServiceImpl.ts",
        "src/test/test.ts"
    ]
}
