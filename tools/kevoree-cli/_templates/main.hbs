{{#is type "node"}}
add node0: {{namespace}}.{{name}}/1/{ js: '0.0.1' }
{{else}}
add node0: JavascriptNode/LATEST/LATEST
  {{#is type "component"}}
add node0.comp: {{namespace}}.{{name}}/1/{ js: '0.0.1' }
  {{else}}
    {{#is type "group"}}
add group: {{namespace}}.{{name}}/1/{ js: '0.0.1' }
attach node0 group
    {{else}}
      {{#is type "channel"}}
add node0.ticker: Ticker/LATEST/LATEST
add node0.printer: ConsolePrinter/LATEST/LATEST
add chan: {{namespace}}.{{name}}/1/{ js: '0.0.1' }

bind node0.ticker.tick chan
bind node0.printer.input chan
      {{/is}}
    {{/is}}
  {{/is}}
{{/is}}
