{
  "name": "language-kevscript",
  "version": "1.0.1",
  "description": "KevScript language support in Atom",
  "main": "lib/main.js",
  "scripts": {
    "publish": "apm publish --tag \"language-kevscript@$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\", ]//g')\""
  },
  "keywords": [
    "kevoree",
    "grammar",
    "kevscript",
    "script",
    "syntax",
    "highlighter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kevoree/kevoree-js.git"
  },
  "license": "LGPL-3.0",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "dependencies": {
    "atom-linter": "^10.0.0",
    "kevoree-const": "^2.0.1",
    "kevoree-kevscript": "^5.0.5",
    "kevoree-logger": "^1.0.1",
    "tiny-conf": "^1.1.0"
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-config-kevoree": "^1.0.1"
  }
}
